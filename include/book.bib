@book{kruschke_doing_2014,
  title = {Doing {{Bayesian Data Analysis}}, {{Second Edition}}: {{A Tutorial}} with {{R}}, {{JAGS}}, and {{Stan}}},
  shorttitle = {Doing {{Bayesian Data Analysis}}, {{Second Edition}}},
  author = {Kruschke, John K.},
  year = {2014},
