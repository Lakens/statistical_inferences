  shorttitle = {Norms and {{Counter-Norms}} in a {{Select Group}} of the {{Apollo Moon Scientists}}},
  author = {Mitroff, Ian I.},
  year = {1974},
  journal = {American Sociological Review},
  volume = {39},
  number = {4},
